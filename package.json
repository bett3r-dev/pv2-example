  "scripts": {
    "clean": "git clean -fXd -e \\!node_modules -e \\!node_modules/**/*",
    "prebuild": "yarn clean",
    "build": "tsc -p tsconfig.build.json && mkdir ./build/config && cp src/config/default.yaml ./build/config/",
